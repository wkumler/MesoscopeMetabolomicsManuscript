@article{Mojzes2020,
  title = {Guanine, a High-Capacity and Rapid-Turnover Nitrogen Reserve in Microalgal Cells},
  author = {Mojze{\v s}, Peter and Gao, Lu and Ismagulova, Tatiana and Pil{\'a}tov{\'a}, Jana and Moud{\v r}{\'i}kov{\'a}, {\v S}{\'a}rka and Gorelova, Olga and Solovchenko, Alexei and Nedbal, Ladislav and Salih, Anya},
  year = {2020},
