  shorttitle = {Iron Depletion in the Deep Chlorophyll Maximum},
  author = {Hawco, Nicholas J. and Barone, Benedetto and Church, Matthew J. and Babcock-Adams, Lydia and Repeta, Daniel J. and Wear, Emma K. and Foreman, Rhea K. and Bj{\"o}rkman, Karin M. and Bent, Shavonna and Van Mooy, Benjamin A. S. and Sheyn, Uri and DeLong, Edward F. and Acker, Marianne and Kelly, Rachel L. and Nelson, Alexa and Ranieri, John and Clemente, Tara M. and Karl, David M. and John, Seth G.},
  year = {2021},
