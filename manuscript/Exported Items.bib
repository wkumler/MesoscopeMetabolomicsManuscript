  author = {Dugenne, Mathilde and Gradoville, Mary R. and Church, Matthew J. and Wilson, Samuel T. and Sheyn, Uri and Harke, Matthew J. and Bj{\"o}rkman, Karin M. and Hawco, Nicholas J. and Hynes, Annette M. and Ribalet, Fran{\c c}ois and Karl, David M. and DeLong, Edward F. and Dyhrman, Sonya T. and Armbrust, E. Virginia and John, Seth and Eppley, John M. and Harding, Katie and Stewart, Brittany and Cabello, Ana M. and Turk-Kubo, Kendra A. and Caffin, Mathieu and White, Angelicque E. and Zehr, Jonathan P.},
  year = {2023},
